{
  "roscore": {
    "command": "roscore",
    "name": "ROS 1 Core",
    "additional_commands": ["source /opt/ros/noetic/setup.bash"],
    "cwd": null,
    "depends": null,
    "stabilize_time": 5
  },
  "rosbridge_server": {
    "command": "roslaunch rosbridge_server rosbridge_websocket.launch address:=141.219.126.94 port:=9090 authenticate=true",
    "name": "Network Telemetry Server",
    "additional_commands": ["source /opt/ros/noetic/setup.bash"],
    "cwd": null,
    "depends": ["roscore"],
    "stabilize_time": 5
  },
  "glances": {
    "command": "glances -w",
    "name": "Glances System Monitor",
    "additional_commands": null,
    "cwd": null,
    "depends": null,
    "stabilize_time": 5
  },
  "mciu_serial": {
    "command": "rosrun rosserial_python serial_node.py _port:=/dev/ttyACM0 _baud:=4000000",
    "name": "MCIU Serial",
    "additional_commands": ["source /opt/ros/noetic/setup.bash"],
    "cwd": null,
    "depends": ["roscore"],
    "stabilize_time": 5
  },
  "adau_serial": {
    "command": "rosrun rosserial_server serial_node _port:=/dev/ttyACM1 _baud:=115200",
    "name": "ADAU Serial",
    "additional_commands": ["source /opt/ros/noetic/setup.bash"],
    "cwd": null,
    "depends": ["roscore"],
    "stabilize_time": 5
  }
}