{
  "roscore": {
    "command": "roscore",
    "name": "ROS 1 Core",
    "additional_commands": ["source /opt/ros/noetic/setup.bash"],
    "cwd": null,
    "depends": null,
    "stabilize_time": 5
  },
  "rosbridge_server": {
    "command": "roslaunch rosbridge_server rosbridge_websocket.launch address:={address} port:=9090",
    "name": "Network Telemetry Server",
    "additional_commands": [
      "source /opt/ros/noetic/setup.bash",
      "source /home/jetson/ros_ws/devel/setup.sh"
    ],
    "cwd": null,
    "depends": ["roscore"],
    "stabilize_time": 5
  },
  "glances": {
    "command": "glances -w",
    "name": "Glances System Monitor",
    "additional_commands": null,
    "cwd": null,
    "depends": null,
    "stabilize_time": 5
  },
  "mciu_serial": {
    "command": "rosrun rosserial_python serial_node.py _port:=/dev/ttyACM0 _baud:=4000000",
    "name": "MCIU Serial",
    "additional_commands": ["source /opt/ros/noetic/setup.bash"],
    "cwd": null,
    "depends": ["roscore", "rosbridge_server"],
    "stabilize_time": 5
  },
  "qmc": {
    "command": "rosrun primrose_qmc qmc",
    "name": "Quarter Module Controller",
    "additional_commands": [
      "source /opt/ros/noetic/setup.bash",
      "source /home/jetson/ros_ws/devel/setup.sh"
    ],
    "cwd": null,
    "depends": [
      "roscore",
      "rosbridge_server",
      "mciu_serial",
      "no_run"
    ],
    "stabilize_time": 5
  },
  "trch": {
    "command": "rosrun primrose_trch trch",
    "name": "Trencher Controller",
    "additional_commands": [
      "source /opt/ros/noetic/setup.bash",
      "source /home/jetson/ros_ws/devel/setup.sh"
    ],
    "cwd": null,
    "depends": [
      "roscore",
      "rosbridge_server",
      "mciu_serial"
    ],
    "stabilize_time": 5
  },
  "conv": {
    "command": "rosrun primrose_conv conv",
    "name": "Conveyor Controller",
    "additional_commands": [
      "source /opt/ros/noetic/setup.bash",
      "source /home/jetson/ros_ws/devel/setup.sh"
    ],
    "cwd": null,
    "depends": [
      "roscore",
      "rosbridge_server",
      "mciu_serial"
    ],
    "stabilize_time": 5
  },
  "imu": {
    "command": "roslaunch /home/jetson/ros_ws/src/ros-imu-bno055/launch/imu.launch",
    "name": "IMU",
    "additional_commands": [
      "source /opt/ros/noetic/setup.bash",
      "source /home/jetson/ros_ws/devel/setup.sh"
    ],
    "cwd": null,
    "depends": [
      "roscore",
      "rosbridge_server",
      "mciu_serial"
    ],
    "stabilize_time": 5
  }
}